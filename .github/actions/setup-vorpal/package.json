{
    "name": "setup-vorpal-action",
    "version": "1.0.0",
    "description": "GitHub Action to setup Vorpal",
    "main": "dist/index.js",
    "scripts": {
        "build": "npm run build-index && npm run build-cleanup",
        "build-index": "ncc build src/index.js -o dist --source-map",
        "build-cleanup": "ncc build src/cleanup.js -o dist-cleanup --source-map",
        "package": "npm run build"
    },
    "dependencies": {
        "@actions/core": "^1.11.1",
        "@actions/exec": "^1.1.1",
        "@actions/io": "^1.1.3"
    },
    "devDependencies": {
        "@vercel/ncc": "^0.38.3"
    }
}
